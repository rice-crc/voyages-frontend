{
 "compilerOptions": {
  "types": ["@types/leaflet", "dotenv"],
   "target":"ES2022",
   "jsx":"react-jsx",
   "module":"ES2022",
   "moduleResolution":"node",
   "esModuleInterop": true,
   "forceConsistentCasingInFileNames": true,
   "strict": true,
   "skipLibCheck": true,
   "resolveJsonModule": true,
   "baseUrl": ".",
   "paths": {
     "@/*": ["src/*"]
   }
  }
}
